{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "completed": {
        "type": "boolean",
        "description": "Indicates whether the task is completed",
        "example": false
      },
      "created": {
        "type": "string",
        "format": "date-time",
        "description": "Timestamp when the task was created",
        "example": "2024-09-23T21:38:07-04:00"
      },
      "details": {
        "type": "string",
        "description": "Details about the task",
        "example": "#1: Turning in your CSC305 Assignment 3"
      },
      "title": {
        "type": "string",
        "description": "Title of the task",
        "example": "#1: Turning in your CSC305 Assignment 3"
      },
      "user": {
        "type": "string",
        "format": "uri",
        "description": "URI of the user associated with the task",
        "example": "/users/rXznsPDl5gMiORKFXdTvY1uaVYB2"
      }
    },
    "required": ["completed", "created", "details", "title", "user"],
    "additionalProperties": false
  }