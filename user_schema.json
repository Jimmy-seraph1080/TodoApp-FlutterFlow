{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "user": {
        "type": "object",
        "properties": {
          "birthday": {
            "type": "string",
            "format": "date-time",
            "description": "User's birthday in ISO 8601 format",
            "example": "2024-09-01T00:00:00-04:00"
          },
          "created_time": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp when the user record was created",
            "example": "2024-09-23T21:06:19-04:00"
          },
          "display_name": {
            "type": "string",
            "description": "The display name of the user",
            "example": "Jimmy Zhang"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "User's email address",
            "example": "jimmy_zhang@uri.edu"
          },
          "photo_url": {
            "type": "string",
            "format": "uri",
            "description": "URL pointing to the user's profile photo",
            "example": "https://firebasestorage.googleapis.com/v0/b/to-do-n0r7hn.appspot.com/o/users%2F1AGSGBjJKMYRC3iWb2BbPzKj8Mh1%2Fuploads%2F1727140019230000.png?alt=media&token=d14b426e-9559-4684-b475-f353c6c89dee"
          },
          "uid": {
            "type": "string",
            "description": "Unique identifier for the user",
            "example": "1AGSGBjJKMYRC3iWb2BbPzKj8Mh1"
          }
        },
        "required": ["birthday", "created_time", "display_name", "email", "photo_url", "uid"],
        "additionalProperties": false
      }
    },
    "required": ["user"],
    "additionalProperties": false
  }